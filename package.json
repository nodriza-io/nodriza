  "bugs": {
    "url": "https://github.com/nodriza-io/nodriza/issues"
  },
  "homepage": "https://github.com/nodriza-io/nodriza#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "lodash": "^4.17.4",
    "numeral": "^2.0.6",
    "query-string": "^5.0.0",
    "socket.io-client": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^4.4.0",    
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0"
